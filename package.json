{
	"name": "@tinted/opus",
	"version": "0.0.1",
	"description": "Opus bindings for Node",
	"licenses": [
		{
			"type": "MIT"
		}
	],
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"types": "typings/index.d.ts",
	"license": "MIT",
	"files": [
		"lib",
		"build/node-opus.node",
		"typings",
		"README.md"
	],
	"scripts": {
		"build": "bun build.ts",
		"lint": "bun x --bun prettier --check . && eslint lib --format=pretty",
		"lint:fix": "bun x --bun prettier --write . && eslint lib --fix --format=pretty",
		"test": "bun test"
	},
	"keywords": [
		"native",
		"opus",
		"module",
		"c",
		"c++",
		"bindings",
		"binary"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/theoparis/discordjs-opus.git"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/bun": "^1.2.6",
		"@types/node": "^22.13.13",
		"eslint": "^9.23.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"engines": {
		"node": ">=12.0.0"
	}
}
